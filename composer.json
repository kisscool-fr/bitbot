{
    "name": "kisscool/bitbot",
    "description": "Bitbot. I'm a bit. Yes. No",
    "homepage": "https://bitbot.kisscool.net",
    "license": "MIT",
    "authors": [
        {
            "name": "KisSCoOl",
            "email": "kisscool@kisscool.net",
            "role": "Developer"
        }
    ],
    "config": {
        "bin-dir": "bin/",
        "optimize-autoloader": true,
        "platform": {
            "php": "7.2"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Bitbot\\": "lib/"
        }
    },
    "require": {
        "php": "^7.2",
        "curl/curl": "^1.8",
        "hirak/prestissimo": "^0.3.8",
        "lokhman/silex-config": "^2.0",
        "monolog/monolog": "^1.23",
        "silex/silex": "^2.2",
        "studio24/rotate": "^1.0",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^7.5",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpstan/phpstan": "^0.11"
    }
}
