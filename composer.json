{
    "name": "kisscool/bitbot",
    "description": "Bitbot. I'm a bit. Yes. No",
    "homepage": "https://bitbot.kisscool.net",
    "license": "MIT",
    "authors": [
        {
            "name": "KisSCoOl",
            "email": "kisscool@kisscool.net",
            "role": "Developer"
        }
    ],
    "config": {
        "bin-dir": "bin/",
        "optimize-autoloader": true,
        "platform": {
            "php": "7.3.27"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Bitbot\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bitbot\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3",
        "curl/curl": "^2.3",
        "lokhman/silex-config": "^2.0",
        "monolog/monolog": "^1.26",
        "silex/silex": "^2.3",
        "studio24/rotate": "^1.0",
        "vlucas/phpdotenv": "^4.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.19",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "^5.3",
        "symfony/css-selector": "^5.3"
    }
}
